{
    "name": "zfmaster/validators",
    "type": "project",
    "license": "BSD-3-Clause",
    "description": "A set of supplemental validators for Zend Framework",
    "homepage": "https://github.com/zfmastery/validators",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Matthew Setter",
            "email": "matthew@matthewsetter.com",
            "homepage": "http://matthewsetter.com",
            "role": "developer"
        }
    ],
    "support": {
        "email": "matthew@matthewsetter.com"
    },
    "require": {
        "php": ">=5.6",
        "zendframework/zend-validator": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "friendsofphp/php-cs-fixer": "^1.11",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "ZfMastery\\Validators\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZfMasteryTest\\Validators\\": "test/"
        }
    }
}
